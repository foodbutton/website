{
  "name": "foodbutton",
  "version": "0.0.0",
  "description": "",
  "homepage": "https://github.com/foodbutton/website",
  "repository": {
    "type": "git",
    "url": "https://github.com/foodbutton/website.git"
  },
  "bugs": {
    "url": "https://github.com/foodbutton/issues"
  },
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  },
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached --hot",
    "build": "rimraf dist && webpack --webpack-build --progress --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --inline --progress --colors --display-error-details --display-cached --hot --port=3000",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "test": "karma start --webpack-test",
    "test-watch": "karma start --webpack-test --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "angular2-jwt": "^0.1.3",
    "brfs": "^1.4.1",
    "ejsify": "^1.0.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.2.0",
    "json-loader": "^0.5.4",
    "ng2-material": "^0.1.0",
    "packageify": "^0.2.3",
    "rxjs": "5.0.0-beta.0",
    "transform-loader": "^0.2.3",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.3.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
